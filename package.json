{
  "name": "thematic-frontend-task",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel spa api-server:dev",
    "test": "jest --passWithNoTests",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "spa": "react-scripts start",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "api-server": "node api-server.js",
    "api-server:dev": "nodemon api-server.js",
    "build": "react-scripts build",
    "prod": "npm-run-all --parallel server api-server"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/auth0-js": "^9.14.0",
    "@types/node": "^12.12.62",
    "@types/react": "^16.9.50",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/reactstrap": "^8.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "helmet": "^3.18.0",
    "highlight.js": "^10.0.3",
    "jwks-rsa": "^1.4.0",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.4.3",
    "reactstrap": "^8.0.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.4.1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parserOptions": {
      "ecmaVersion": "9"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
